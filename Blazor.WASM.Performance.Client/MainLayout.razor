@inherits LayoutComponentBase
@using MudBlazor

<MudThemeProvider Theme="_defaultTheme"/>

<MudLayout>
    <MudAppBar>
        <MudText Typo="Typo.h4">
            Blazor WebAssembly Performance
        </MudText>
    </MudAppBar>
    <MudMainContent>
        <MudContainer MaxWidth="MaxWidth.ExtraLarge" Style="padding: 1.5rem; height: calc(100vh - var(--mud-appbar-height));">
            @Body
        </MudContainer>
    </MudMainContent>
</MudLayout>


